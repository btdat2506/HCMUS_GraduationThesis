  \strng{fullhash}{JNPYCPNPDAGBRBSBSBNBABRCPlCCCCCJDMDMDJGBGTVGRGWHRHCRHDHJHRHDIJJAJAKAKHKDKAKNLSLJLJLDLCLZLKLAMGMAMMMKNRNRNRNKNTOMPNPARJRMSASESCSGSMSJSDSATMTGTBTHTEVVWRWWWEYDH1}
